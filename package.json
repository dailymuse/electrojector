{
  "name": "@themuse/electrojector",
  "version": "0.1.1",
  "description": "Simple dependency injection for JS",
  "main": "index.js",
  "author": "Daily Muse, Inc.",
  "license": "MIT",
  "devDependencies": {
    "@themuse/muse-lint-tslint": "~0.3.0",
    "ava": "~1.3.1",
    "codecov": "~3.2.0",
    "husky": "~1.3.1",
    "lint-staged": "~8.1.5",
    "nyc": "~13.3.0"
  },
  "dependencies": {
    "@themuse/conflator": "~0.1.0",
    "sinon": "~7.2.7"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "sources": [
      "index.js",
      "src/*.js",
      "test/config/**/*",
      "test/dependencies/**/*"
    ]
  },
  "scripts": {
    "coverage": "./bin/local-coverage",
    "lint:ts": "tslint -c ./tslint.json '*.{tsx,ts,jsx,js}' '*/*.{tsx,ts,jsx,js}'",
    "lint:ts:fix": "tslint -c ./tslint.json '*.{tsx,ts,jsx,js}' '*/*.{tsx,ts,jsx,js}' --fix",
    "test": "./bin/local-test"
  },
  "lint-staged": {
    "**/*.{tsx,ts,jsx,js}": [
      "npm run lint:ts --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
